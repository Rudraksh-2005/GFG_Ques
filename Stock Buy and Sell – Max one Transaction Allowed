class Solution {
  public:
    int maxProfit(vector<int> &prices) {
        // code here
        int mini=prices[0];
        int maxi=0;
        
        for(int i=1;i<prices.size();i++){
            maxi=max(maxi,prices[i]-mini);
            if(prices[i]<mini){
                mini=prices[i];
            }
        }
        
        return maxi;
    }
};
